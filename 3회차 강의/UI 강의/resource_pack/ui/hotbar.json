{
    "namespace": "hotbar",
    "hotbar_group": {
        "type": "panel",
        "size": [
            240,
            22
        ],
        "anchor_from": "center",
        "anchor_to": "center",
        "controls": [
            {
                "hotbar_g1@hotbar_element": {
                    "$offset": [
                        96,
                        0
                    ],
                    "offset": [
                        "-100%c",
                        "-100% + 200%c"
                    ],
                    "$texture": "textures/watts/hotbar_1",
                    "$selected_image": "textures/watts/select"
                }
            },
            {
                "hotbar_g2@hotbar_element": {
                    "$offset": [
                        72,
                        0
                    ],
                    "offset": [
                        "100%c",
                        "-100% + 200%c"
                    ],
                    "$texture": "textures/watts/hotbar_2",
                    "$selected_image": "textures/watts/select"
                }
            },
            {
                "hotbar_g3@hotbar_element": {
                    "$offset": [
                        48,
                        0
                    ],
                    "offset": [
                        "50% - 100%c",
                        "50%c"
                    ],
                    "$texture": "textures/watts/hotbar_3"
                }
            },
            {
                "hotbar_g4@hotbar_element": {
                    "$offset": [
                        24,
                        0
                    ],
                    "offset": [
                        "50% - 100%c",
                        "-50%c"
                    ],
                    "$texture": "textures/watts/hotbar_4"
                }
            },
            {
                "hotbar_g5@hotbar_element": {
                    "$offset": [
                        0,
                        0
                    ],
                    "offset": [
                        "-150%c",
                        "0"
                    ],
                    "$texture": "textures/watts/hotbar_5"
                }
            },
            {
                "hotbar_g6@hotbar_element": {
                    "$offset": [
                        -24,
                        0
                    ],
                    "offset": [
                        "-50%c",
                        0
                    ],
                    "$texture": "textures/watts/hotbar_6"
                }
            },
            {
                "hotbar_g7@hotbar_element": {
                    "$offset": [
                        -48,
                        0
                    ],
                    "offset": [
                        "50%c",
                        0
                    ],
                    "$texture": "textures/watts/hotbar_7"
                }
            },
            {
                "hotbar_g8@hotbar_element": {
                    "$offset": [
                        -72,
                        0
                    ],
                    "offset": [
                        "-50% + 100%c",
                        "-50%c"
                    ],
                    "$texture": "textures/watts/hotbar_8"
                }
            },
            {
                "hotbar_g9@hotbar_element": {
                    "$offset": [
                        -96,
                        0
                    ],
                    "offset": [
                        "-50% + 100%c",
                        "50%c"
                    ],
                    "$texture": "textures/watts/hotbar_9"
                }
            }
        ]
    },
    "hotbar_element": {
        "type": "panel",
        "anchor_from": "center",
        "anchor_to": "center",
        "clips_children": true,
        "size": [
            24,
            22
        ],
        "controls": [
            {
                "hotbar_ele1@hotbar_ele": {
                    "offset": "$offset"
                }
            }
        ]
    },
    "hotbar_ele": {
        "size": [
            216,
            22
        ],
        "type": "panel",
        "anchor_from": "center",
        "anchor_to": "center",
        "controls": [
            {
                "hotbar_grid_ele@hud.hotbar_grid": {
                    "grid_item_template": "hotbar.gui_hotbar_grid_item",
                    "size": [
                        216,
                        22
                    ]
                }
            }
        ]
    },
    "gui_hotbar_grid_item@hud.gui_hotbar_grid_item": {
        "controls": [
            {
                "hotbar_slot_selected_image@hotbar_slot_selected_image": {}
            },
            {
                "hotbar_renderer@hotbar.hotbar_renderer": {}
            },
            {
                "hotbar_parent": {
                    "type": "panel",
                    "size": [
                        18,
                        18
                    ],
                    "controls": [
                        {
                            "item_icon@hud.hotbar_hud_item_icon": {
                                "layer": 3,
                                "size": [
                                    16,
                                    16
                                ]
                            }
                        },
                        {
                            "item_count@common.stack_count_label": {
                                "layer": 4,
                                "$item_collection_name": "$hotbar_collection_name",
                                "$stack_count_collection_name": "$hotbar_collection_name",
                                "bindings": [
                                    {
                                        "binding_name": "#inventory_stack_count",
                                        "binding_type": "collection",
                                        "binding_collection_name": "$stack_count_collection_name"
                                    },
                                    {
                                        "binding_name": "#stack_count_visible",
                                        "binding_name_override": "#visible",
                                        "binding_type": "collection",
                                        "binding_collection_name": "$stack_count_collection_name"
                                    }
                                ]
                            }
                        },
                        {
                            "hotbar_hint@hud.edu_common.hotbar_hint": {}
                        },
                        {
                            "item_dura@hud.item_durability": {
                                "layer": 5
                            }
                        },
                        {
                            "item_store@hud.item_storage": {
                                "layer": 5
                            }
                        },
                        {
                            "container_item_lock_overlay@hud.container_item_lock_overlay": {
                                "layer": 6,
                                "size": [
                                    16,
                                    16
                                ]
                            }
                        }
                    ]
                }
            },
            {
                "cooldown_renderer@hud.cooldown_renderer": {
                    "layer": 10
                }
            },
            {
                "hotbar_slot_button@hud.gui_hotbar_slot_button_prototype": {}
            }
        ]
    },
    "hotbar_renderer@hud.hotbar_renderer": {
        "controls": [
            {
                "hotbar_slot_image@hotbar.hotbar_slot_image": {
                    "$hotbar_slot_image_size": "$hotbar_renderer_size"
                }
            }
        ]
    },
    "hotbar_slot_image": {
        "$hotbar_slot_image_size|default": [
            20,
            22
        ],
        "texture": "$texture",
        "type": "image",
        "offset": [
            0,
            0
        ],
        "size": "$hotbar_slot_image_size",
        "layer": 1,
        "alpha": 0.65
    }
}